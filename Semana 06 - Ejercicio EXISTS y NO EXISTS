--USANDO EXISTS
SELECT nombre
FROM Empleados E
WHERE EXISTS (
    SELECT 1
    FROM Proyectos P
    WHERE P.empleado_id = E.empleado_id
);

--USANDO NO EXISTS
SELECT nombre
FROM Empleados E
WHERE NOT EXISTS (
    SELECT 1
    FROM Proyectos P
    WHERE P.empleado_id = E.empleado_id
);
