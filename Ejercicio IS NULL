CREATE TABLE Empleados (
    ID INT PRIMARY KEY,
    Nombre VARCHAR(50),
    SalarioBase DECIMAL(10, 2),
    Bono DECIMAL(10, 2) NULL
);

INSERT INTO Empleados (ID, Nombre, SalarioBase, Bono) VALUES
(1, 'Ana', 3000.00, 500.00),
(2, 'Luis', 3200.00, NULL),
(3, 'Mar√≠a', 2800.00, 300.00),
(4, 'Fernando', 3500.00, NULL);

SELECT 
    ID,
    Nombre,
    SalarioBase,
    Bono
FROM 
    Empleados
WHERE 
    Bono IS NULL;
